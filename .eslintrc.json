{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-restricted-syntax": [
      "error",
      "FunctionExpression",
      "WithStatement",
      "BinaryExpression[operator='in']"
    ]
  }
}
